//4. numbers should not be divisible by 3 & should not end with 3.
t=int(input("Enter the no of test-cases"))
for _ in range(t):
    k=int(input("Enter the value of k:"))
    count=0
    while(1):
        if i%3!=0 and i%10!=3:
            count+=1
        if count==k:
            print(i)
